<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>net7.0</TargetFramework>
    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>
    <GeneratePackageOnBuild>True</GeneratePackageOnBuild>
    <Title>Albon.SessionTokenProvider</Title>
    <Copyright>MIT</Copyright>
    <PackageProjectUrl>https://github.com/alex892-droid/Albon.SessionTokenProvider</PackageProjectUrl>
    <RepositoryUrl>https://github.com/alex892-droid/Albon.SessionTokenProvider</RepositoryUrl>
    <RepositoryType>git</RepositoryType>
    <PackageTags>session; token; provider</PackageTags>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Albon.CryptographyTools" Version="1.0.3" />
    <PackageReference Include="BasicLinkedObjectBase" Version="1.0.1" />
    <PackageReference Include="Microsoft.IdentityModel.Tokens" Version="6.31.0" />
    <PackageReference Include="Newtonsoft.Json" Version="13.0.3" />
  </ItemGroup>

</Project>
